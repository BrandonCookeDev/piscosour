{
  "cmd": "pisco",
  "analytics": {
    "id": "UA-23096868-2",
    "ask": true
  },
  "params": {
    "workingDir": "",
    "plugins": [
      "os",
      "context",
      "launcher",
      "inquirer",
      "fsutils",
      "piscosour",
      "system-checker",
      "system-saver",
      "skipper"
    ],
    "versions": {
      "bower" : {
        "list": "bower cache list"
      },
      "npm" : {
        "disableCheck": true,
        "npm": true,
        "list": "npm list -g --depth 0",
        "regexp": "\\@(.*?)\\s"
      },
      "yo": {
        "option": "--version"
      },
      "java": {
        "option": "-version",
        "regexp": "\"(.*?)_"
      },
      "sass": {
        "regexp": "s (.*?) "
      },
      "git": {
        "option": "--version",
        "regexp": "n (.*?)\\n"
      }
    },
    "contexts": {
      "node-module": [
        {
          "file": "package.json",
          "conditions": [
            "that.version"
          ]
        },
        {
          "file": "piscosour.json",
          "noexists": "true"
        }
      ],
      "recipe": [
        {
          "sufficient": true,
          "file": ".piscosour/piscosour.json",
          "conditions": [
            "that.repoType==='recipe'"
          ]
        },
        {
          "file": "package.json",
          "conditions": [
            "that.keywords && that.keywords.indexOf('piscosour-recipe')>=0"
          ]
        },
        {
          "file": "piscosour.json"
        }
      ]
    }
  },
  "defaultType": "recipe",
  "repoTypes": [
    "recipe",
    "node-module"
  ],
  "junitDir": "test-reports",
  "junitPiscoFile": "pisco-junit.xml",
  "stages": [
    "check",
    "config",
    "run",
    "prove",
    "notify",
    "emit"
  ],
  "strawTypes": [
    "normal",
    "utils",
    "internal"
  ]
}