{
  "cmd": "pisco",
  "analytics": {
    "id": "UA-23096868-2",
    "ask": true
  },
  "params": {
    "workingDir": "",
    "plugins": [
      "os",
      "context",
      "launcher",
      "inquirer",
      "fsutils",
      "piscosour",
      "installer",
      "skipper",
      "system-checker",
      "system-saver"
    ],
    "versions": {
      "bower" : {
        "installer": "npm",
        "list": "bower cache list",
        "cmdInstaller": "bower install"
      },
      "npm" : {
        "list": "npm list -g --depth 0",
        "regexp": "\\@(.*?)\\s",
        "cmdInstaller": "npm install -g"
      },
      "yo": {
        "installer": "npm",
        "option": "--version"
      },
      "java": {
        "option": "-version",
        "regexp": "\"(.*?)_"
      },
      "sass": {
        "regexp": "s (.*?) "
      },
      "git": {
        "option": "--version",
        "regexp": "n (.*?)\\n"
      }
    }
  },
  "defaultContext": "recipe",
  "junitDir": "test-reports",
  "junitPiscoFile": "pisco-junit.xml",
  "stages": [
    "core-install",
    "core-check",
    "check",
    "config",
    "run",
    "prove",
    "notify",
    "emit"
  ],
  "flowTypes": [
    "normal",
    "utils",
    "internal"
  ]
}
